#include <iostream>
using namespace std;

int  main()
{
	char  op; 
	int  num1, num2, result, i=1, N;
	cin>>N; 
	while(i<=N)
	{
	cin >> num1 >> op >> num2;
	switch (op)
	{  
		case '+' : result =num1+num2; break;
		case '-' : result =num1-num2; break;
		case '*' : result =num1*num2; break;
		case '/' : result =num1/num2; break;
		case '0' :break;
		default: cout << "运算符非法." << endl;
	} 
	
	cout << num1 << op << num2 << "=" << result << endl;
	i++;
	}
	return 0;
}
